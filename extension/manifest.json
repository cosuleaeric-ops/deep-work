{
  "manifest_version": 3,
  "name": "Elite Deep Work – Block social",
  "version": "1.1.0",
  "description": "Blochează Facebook, X, Reddit, Instagram și LinkedIn cât timp timerul de deep work rulează.",
  "permissions": ["storage", "declarativeNetRequest"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
